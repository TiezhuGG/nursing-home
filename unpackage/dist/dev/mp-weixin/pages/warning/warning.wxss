.warning {
  height: 100%;
  background-color: #F7F7F7;
}
.warning .top-nav {
    height: 96rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    margin-bottom: 30rpx;
    background-color: #fff;
    box-shadow: 0rpx 1rpx 0rpx 0rpx #DDDDDD;
}
.warning .top-nav .nav-item {
      padding: 28rpx 0;
      font-size: 28rpx;
      color: #999;
}
.warning .top-nav .nav-item:nth-child(1) {
      margin-left: 146rpx;
}
.warning .top-nav .nav-item:nth-child(2) {
