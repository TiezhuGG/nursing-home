<view class="workbench"><view class="userinfos"><image class="avatar _img" src="{{nurseInfo.avatar}}"></image><view class="infos"><text class="name">{{nurseInfo.name}}</text><view class="job"><image class="medal _img" src="../../static/images/medal.png"></image><text class="job-title">护士</text></view></view></view><block wx:for="{{noticeList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block><view class="notice"><image class="notice-img _img" src="../../static/images/notice.png"></image><text data-event-opts="{{[['tap',[['toNotice',['$0'],[[['noticeList','id',item.id,'id']]]]]]]}}" class="notice-txt" bindtap="__e">{{item.title}}</text></view></block></block><view class="main"><view data-event-opts="{{[['tap',[['toHeartRate',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img _img" src="../../static/images/item-img1.png"></image><text class="item-txt">实时心率</text></view><navigator class="item" url="../heart-rate/heart-rate"><image class="item-img _img" src="../../static/images/item-img2.png"></image><text class="item-txt">心率回放</text></navigator><navigator class="item" url="../electrocardiogram/electrocardiogram"><image class="item-img _img" src="../../static/images/heart.png"></image><text class="item-txt">心电图</text></navigator><view data-event-opts="{{[['tap',[['toBloodStatus',['$event']]]]]}}" class="item" bindtap="__e"><image class="item-img _img" src="../../static/images/item-img4.png"></image><text class="item-txt">血压 血氧 血糖</text></view><navigator class="item" url="../video/video"><image class="item-img _img" src="../../static/images/item-img5.png"></image><text class="item-txt">视频监控</text></navigator><navigator class="item"><image class="item-img _img" src="../../static/images/item-img6.png"></image><text class="item-txt">起夜/摔倒警报</text></navigator></view></view>